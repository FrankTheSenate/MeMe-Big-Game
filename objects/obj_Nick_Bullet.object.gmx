        y+= sign(vspd)
    }
    vspd = 0
}
y+= vspd

//horizontal touchwall
if (place_meeting(x+hspd, y, obj_grass))
{
    while(!place_meeting(x+sign(hspd), y, obj_grass))
    {
        x+= sign(hspd)
    }
    hspd = 0
}
x+= hspd


//Aminations
if hspd &gt; 0 and vspd = 0
{
sprite_index = spr_Nick_Bullet_Walking
image_xscale = -1
}
else if hspd &lt; 0 and vspd = 0
{
sprite_index = spr_Nick_Bullet_Walking
image_xscale = 1
}
else if hspd = 0 and vspd = 0
{
image_index = spr_Nick_Bullet_Idle
}
else if hspd and vspd = 0
{
image_index = spr_Nick_Bullet_Idle
}

</string>
